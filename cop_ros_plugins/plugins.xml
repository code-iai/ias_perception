<library path="lib/libCopRosPlugin">
  <class name="ImageSubscription" type="cop::ImageSubscription" base_class_type="Sensor">
    <description>
      This is a plugin to make the swissranger available as a remote sensor
      in cop.
    </description>
  </class>
  <class name="DetectedFace" type="cop::DetectedFace"
  base_class_type="Descriptor">
    <description>
      plugin representing cluster and handles
    </description>
  </class>
  <class name="TableObjectDescriptor" type="cop::TableObjectDescriptor"
  base_class_type="Descriptor">
    <description>
      Descriptor to store table object message
    </description>
  </class>
  <class name="TableObjectAttention" type="cop::TableObjectAttention"
    base_class_type="AttentionAlgorithm">
    <description>
       Collects resulting table objects
    </description>
  </class>
  <class name="FaceDetection" type="cop::FaceDetection"
    base_class_type="LocateAlgorithm">
    <description>
       Detect clusters on planes in the swissranger data.
    </description>
  </class>
  <class name="TableObjectRedetector" type="cop::TableObjectRedetector"
    base_class_type="LocateAlgorithm">
    <description>
       Redetecs table objects
    </description>
  </class>

  <!--Readings -->
    <class name="IplImageReading" type="cop::IplImageReading" base_class_type="Reading">
        <description>
              This is a plugin reads an image file
        </description>
    </class>
</library>
